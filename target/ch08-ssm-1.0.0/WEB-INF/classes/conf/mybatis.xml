<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!--设置别名-->
    <typeAliases>
        <!--name是实体类所在的包名-->
        <package name="top.hr37.domain"/>
    </typeAliases>

    <!--使用package接口名和mapper文件名必须一致，必须在同一目录-->
    <mappers>
        <package name="top.hr37.dao"/>
    </mappers>
</configuration>